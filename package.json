{
  "name": "@ai-lion/liondb",
  "version": "3.7.6",
  "main": "dist/node.js",
  "license": "MIT",
  "bin": {},
  "types": "index.d.ts",
  "scripts": {
    "build": "run-s build:node build:browser",
    "build:node": "webpack --mode=production --node-env=production",
    "build:browser": "webpack --config webpack.config.browser.js --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "test": "mocha 'tests/**/*.ts' ",
    "watch": "webpack --watch",
    "serve": "webpack serve",
    "format": "run-s format:*",
    "format:js": " prettier --write   \"src/**/*.ts\"  "
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "keywords": [
    "liondb",
    "leveldb",
    "cache",
    "db",
    "nosql",
    "数据库",
    "本地数据库",
    "浏览器数据库"
  ],
  "repository": "https://github.com/ai-lion/liondb",
  "dependencies": {
    "buffer": "^6.0.3",
    "level-js": "^6.0.0",
    "leveldown": "^5.6.0",
    "levelup": "^4.4.0"
  },
  "devDependencies": {
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.4.2",
    "@webpack-cli/generators": "^2.2.0",
    "assert": "^2.0.0",
    "autoprefixer": "^10.3.1",
    "copy-webpack-plugin": "^9.0.1",
    "mocha": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "process": "^0.11.10",
    "ts-loader": "^9.2.4",
    "ts-node": "^10.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.5",
    "util": "^0.12.4",
    "webpack": "^5.46.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "workbox-webpack-plugin": "^6.1.5"
  },
  "description": "liondb是一个基于leveldb的本地磁盘存储数据库， 数据可以持久存储， 性能高效， 容量受限于磁盘大小",
  "mocha": {
    "require": [
      "ts-node/register"
    ],
    "ui": "bdd"
  }
}
