    "node-machine-id": "^1.1.12"
  },
  "devDependencies": {
    "@types/mocha": "^8.2.2",
    "babel-eslint": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "commander": "^4.0.1",
    "copy-webpack-plugin": "^6.0.1",
    "eslint": "^4.19.1",
    "eslint-config-alloy": "^1.4.1",
